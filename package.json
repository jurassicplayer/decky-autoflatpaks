  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jurassicplayer/decky-autoflatpaks/issues"
  },
  "homepage": "https://github.com/jurassicplayer/decky-autoflatpaks#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
